  "version": "1.0.5",
  "bin": "src/shims/node.mjs",
  "author": "kamataryo <kamataryo@users.noreply.github.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@geolonia/view-geojson": "^0.0.6"
  },
  "scripts": {
    "deno:allow": "echo --allow-env=POLYGON_CLIPPING_MAX_QUEUE_SIZE,POLYGON_CLIPPING_MAX_SWEEPLINE_SEGMENTS --allow-net=www.inaturalist.org",
    "start": "deno run $(npm run --silent deno:allow) ./src/shims/deno.ts"
